{
  "name": "git-bash-api",
  "version": "1.0.0",
  "description": "",
  "main": "server/index.js",
  "scripts": {
    "prebuild": "rm -rf tmp/ && mkdir tmp && cd tmp/ && git clone https://github.com/facebook/react/ && git clone https://github.com/WIVSW/foreign-api.git && cd ../ && rm -rf node_modules && npm i && $(npm bin)/selenium-standalone install && cd client-react && rm -rf node_modules && npm i && npm run build && cd .. && npm run start ./tmp",
    "start": "node server/index.js",
    "start:selenium": "$(npm bin)/selenium-standalone start",
    "test": "npm run test:lint && npm run test:unit && npm run test:e2e",
    "test:unit": "export NODE_ENV=test && mocha --exit --timeout=5000 --recursive test/unit/index.test.js",
    "test:e2e": "$(npm bin)/hermione test/e2e/index.test.js",
    "test:lint": "npm run lint:server && npm run lint:client",
    "lint:server": "$(npm bin)/eslint server test",
    "lint:client": "client-react/node_modules/.bin/eslint client-react/src"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/WIVSW/git-bash-api.git"
  },
